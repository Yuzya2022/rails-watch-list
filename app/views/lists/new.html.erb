<h1>New list</h1>
<%= simple_form_for(@list) do |f| %>

  <div class="form-inputs">
    <%= f.input :name, label: false, placeholder: "List name" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<br>

<div>
  <%= link_to "Back to lists", lists_path %>
</div>
